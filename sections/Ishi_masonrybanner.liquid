<div class="ishimasonrybanner {% if section.settings.banner_layout == 'box' %}page-width{% endif %}">
        <div class="ishimasonrybanner_grid">
          <div class="ishimasonrybanner-left space col-md-4 col-sm-6 col-xs-12">
            <div class="ishimasonrybanner-container banner1">
              <div class="image-container">
                {% if section.settings.image1 != blank %}
                <div class="media" style="padding-bottom: {{ 1 | divided_by: section.settings.image1.aspect_ratio | times: 100 }}%;">
                  {% endif %}
                <a class="ishi-customhover-{{ section.settings.bannerhover1 }} {% if section.settings.banner_scale_enable1 %}scale{% endif %}" 
                   {% if section.settings.banner_link1 != blank %}href="{{ section.settings.banner_link1 }}"{% else %}role="link" aria-disabled="true"{% endif %}>
                 {% if section.settings.image1 != blank %}
                  <img class="lazyload"
                   data-src="{{ section.settings.image1 | img_url: 'master' }}"
                   alt="Banner Image">
                   {% else %}
                      {{ 'image' | placeholder_svg_tag: 'ishi-placeholder' }}
                  {% endif %}
                  {% if section.settings.button_label1 != blank and section.settings.banner_link1 != blank %}
                    <div class="banner-button">
                      <span class="btn">
                        {{ section.settings.button_label1 | escape }}
                      </span>
                    </div>
                  {% endif %}
                 </a>
                  {% if section.settings.image1 != blank %}
                </div>
                {% endif %}
              </div>
            </div>
