<style type="text/css">
  #shopify-section-{{ section.id }} {
    margin-bottom: {{ section.settings.section_bottom_spacing_desktop }}px;
  }
  #shopify-section-{{ section.id }} .space {
    padding: 0 {{ section.settings.banner_leftright_internal_padding | divided_by: 2 }}px;
  }
   #shopify-section-{{ section.id }} .ishimasonrybanner_grid{
    padding: 0 {{ section.settings.banner_leftright_internal_padding | divided_by: 2 }}px;
  }
  #shopify-section-{{ section.id }} .space .ishimasonrybanner-container.banner1,#shopify-section-{{ section.id }} .space .ishimasonrybanner-container.banner4{
    padding-bottom: {{ section.settings.banner_leftright_internal_padding }}px;
  }
  @media (max-width: 767px) {
    #shopify-section-{{ section.id }} {
      margin-bottom: {{ section.settings.section_bottom_spacing_mobile }}px;
    }
    #shopify-section-{{ section.id }} .space .ishimasonrybanner-container.banner4{
       padding-bottom:0; 
       padding-right: {{ section.settings.banner_leftright_internal_padding | divided_by: 2 }}px;
    }
    #shopify-section-{{ section.id }} .space .ishimasonrybanner-container.banner5{
       padding-left: {{ section.settings.banner_leftright_internal_padding | divided_by: 2 }}px;
    }
    #shopify-section-{{ section.id }} .ishimasonrybanner-right{
       padding-top: {{ section.settings.banner_leftright_internal_padding }}px;
    }
  }
  @media (max-width: 543px) {
    #shopify-section-{{ section.id }} .space .ishimasonrybanner-container.banner2{
    padding-bottom: {{ section.settings.banner_leftright_internal_padding }}px;
  }
    #shopify-section-{{ section.id }} .space .ishimasonrybanner-container.banner4{
       padding-bottom: {{ section.settings.banner_leftright_internal_padding }}px;
       padding-right: 0;
    }
    #shopify-section-{{ section.id }} .space .ishimasonrybanner-container.banner5{
       padding-left: 0;
    }
  }
</style>

